{
    "plugins": ["prettier-plugin-tailwindcss"],
    "tabWidth": 4,
    "printWidth": 120,
    "proseWrap": "always",
    "trailingComma": "all",
    "overrides": [
        {
            "files": ["*.md"],
            "options": {
                "proseWrap": "preserve",
                "parser": "markdown"
            }
        }
    ]
